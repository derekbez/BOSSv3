# B.O.S.S. v3 — Secrets Template
# Copy this file to secrets.env and fill in your API keys.
# NEVER commit secrets.env to git.
#
# On the Pi the systemd service loads this file automatically via
# EnvironmentFile=-/opt/boss/secrets/secrets.env

# Aviation (AviationStack) — flights_leaving_heathrow, flight_status_favorite_airline
BOSS_APP_AVIATIONSTACK_API_KEY=

# Bird sightings (eBird) — bird_sightings_near_me
BOSS_APP_EBIRD_API_KEY=

# Geolocation (IPGeolocation) — moon_phase
BOSS_APP_IPGEO_API_KEY=

# Music history (Last.fm) — today_in_music
BOSS_APP_LASTFM_API_KEY=

# Space (NASA APOD) — space_update
BOSS_APP_NASA_API_KEY=

# News (Newsdata.io) — breaking_news
BOSS_APP_NEWSDATA_API_KEY=

# Search trends (SerpApi) — top_trending_search
BOSS_APP_SERPAPI_API_KEY=

# Word of the Day (Wordnik) — word_of_the_day
BOSS_APP_WORDNIK_API_KEY=

# Tide times (WorldTides) — local_tide_times
BOSS_APP_WORLDTIDES_API_KEY=
