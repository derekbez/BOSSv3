{
  "name": "bird_sightings_near_me",
  "display_name": "Bird Sightings Near Me",
  "description": "Recent nearby bird species via eBird API.",
  "version": "1.0.1",
  "author": "BOSS Team",
  "entry_point": "main.py",
  "timeout_seconds": 180,
  "requires_network": true,
  "tags": [
    "content",
    "network"
  ],
  "timeout_behavior": "return",
  "required_env": [
    "BOSS_APP_EBIRD_API_KEY"
  ],
  "config": {
    "radius": 10,
    "per_page": 10,
    "request_timeout_seconds": 6,
    "refresh_seconds": 120
  }
}
